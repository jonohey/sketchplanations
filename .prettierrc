{
  "jsxSingleQuote": true,
  "singleQuote": true,
  "printWidth": 120,
  "semi": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "vanilla-cookieconsent/dist/cookieconsent.css",
    "(.*).css",
    "^(components|helpers|services|hooks|images)(/(.*))?$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
