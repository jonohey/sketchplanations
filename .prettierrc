{
  "jsxSingleQuote": true,
  "singleQuote": true,
  "printWidth": 120,
  "semi": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "importOrder": ["<THIRD_PARTY_MODULES>", "^(components|helpers|services|hooks)(/(.*))?$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
